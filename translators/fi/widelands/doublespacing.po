# (pofilter) doublespacing: Different use of double spaces
#. * TRANSLATORS: Available host command
#: ../../src/network/gamehost.cc:293
msgid "/help  -  Shows this help"
msgstr "/help - Näyttää tämän ohjeen"

# (pofilter) doublespacing: Different use of double spaces
#. * TRANSLATORS: Available host command
#: ../../src/network/gamehost.cc:295
msgid "/announce <msg>  -  Send a chatmessage as announcement (system chat)"
msgstr "/announce <viesti> - Lähettää viestin tiedotteena kaikille"

# (pofilter) doublespacing: Different use of double spaces
#. * TRANSLATORS: Available host command
#: ../../src/network/gamehost.cc:297
msgid "/warn <name> <reason>  -  Warn the user <name> because of <reason>"
msgstr "/warn <nimi> <aihe> - Varoittaa käyttäjää <nimi> aiheesta <aihe>"

# (pofilter) doublespacing: Different use of double spaces
#. * TRANSLATORS: Available host command
#: ../../src/network/gamehost.cc:299
msgid "/kick <name> <reason>  -  Kick the user <name> because of <reason>"
msgstr "/kick <nimi> <syy> - Potkaisee käyttäjän <nimi> ulos aiheesta <aihe>"

# (pofilter) doublespacing: Different use of double spaces
#. * TRANSLATORS: Available host command
#: ../../src/network/gamehost.cc:301
msgid "/forcePause            -  Force the game to pause."
msgstr "/forcePause - Pakottaa pelin pysähtymään."

# (pofilter) doublespacing: Different use of double spaces
#. * TRANSLATORS: Available host command
#: ../../src/network/gamehost.cc:303
msgid "/endForcedPause        -  Return game to normal speed."
msgstr "/endForcedPause - Palauttaa pelin normaaliin pelinopeuteen."

# (pofilter) doublespacing: Different use of double spaces
#: ../../src/network/internet_gaming.cc:117
msgid ""
"Widelands could not establish a connection to the given address.\n"
"Either there was no metaserver running at the supposed port or\n"
"your network setup is broken."
msgstr "Widelands ei saanut verkkoyhteyttä annettuun osoitteeseen.\nJoko  palvelin ei kuuntele oikeaa porttia tai\nverkkoasetuksissasi on jotain vikaa."

# (pofilter) doublespacing: Different use of double spaces
#: ../../src/network/internet_gaming.cc:526
#, c-format
msgid "Invalid chat message type \"%s\"."
msgstr "Virheellinen viestityyppi  \"%s\"."

# (pofilter) doublespacing: Different use of double spaces
#: ../../src/ui_fsmenu/login_box.cc:60
#, c-format
msgid ""
"In order to use a registered account, you need an account on the Widelands "
"website. Please log in at %s and set an online gaming password on your "
"profile page."
msgstr "Rekisteröidyn tunnuksen käyttäminen edellyttää  Widelandsin verkkosivujen tunnusta. Kirjaudu sisään %s ja aseta verkkopelaamisen salasana profiilisivultasi."

# (pofilter) doublespacing: Different use of double spaces
#. * TRANSLATORS: Health, Attack, Defense, Evade
#: ../../src/wui/attack_box.cc:353 ../../src/wui/soldierlist.cc:403
#: ../../src/wui/soldierlist.cc:466
#, c-format
msgid "HP: %1$u/%2$u  AT: %3$u/%4$u  DE: %5$u/%6$u  EV: %7$u/%8$u"
msgstr "HP: %1$u/%2$u AT: %3$u/%4$u DE: %5$u/%6$u EV: %7$u/%8$u"

# (pofilter) doublespacing: Different use of double spaces
#. * TRANSLATORS: Tooltip in the ship statistics window
#: ../../src/wui/seafaring_statistics_menu.cc:527
msgid "Show expeditions that have found a port space or are founding a colony"
msgstr "Näytä tutkimusmatkat, jotka ovat löytäneet satamapaikan tai löytämässä  yhdyskunnan"
