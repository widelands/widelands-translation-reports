msgstr[0] "%1$u peco de nekonata varo."
msgstr[1] "%1$u pecoj de nekonataj varoj."

# (pofilter) endpunc: Different punctuation at the end
#: ../../../../src/map_io/map_saver.cc:87
msgid "Autosaving map…"
msgstr "Aŭtomatsavante la mapon..."

# (pofilter) endpunc: Different punctuation at the end
#: ../../../../src/ui_fsmenu/addons/manager.cc:539
msgid "Move selected add-on to top"
msgstr "Movu la elektitan kromprogramon al la supro."

# (pofilter) endpunc: Different punctuation at the end
