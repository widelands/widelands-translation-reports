# (pofilter) options: Consider translating parameter 'DIRNAME' of option '--datadir'
#: ../../src/wlapplication_messages.cc:48
msgid ""
" --datadir=DIRNAME    Use specified directory for the widelands\n"
"                      data files"
msgstr " --datadir=DIRNAME    Utilise le dossier spécifié pour les données\nde Widelands."

# (pofilter) options: Consider translating parameter 'DIRNAME' of option '--homedir'
#: ../../src/wlapplication_messages.cc:51
msgid ""
" --homedir=DIRNAME    Use specified directory for widelands config\n"
"                      files, savegames and replays"
msgstr " --homedir=DIRNAME    Utilise le dossier spécifié pour la sauvegarde des fichiers de configuration de Widelands,\n sauvegardes et replays."

# (pofilter) options: Consider translating parameter 'DIRNAME' of option '--localedir'
#: ../../src/wlapplication_messages.cc:59
msgid ""
" --localedir=DIRNAME  Use specified directory for the widelands\n"
"                      locale files"
msgstr " --localedir=DIRNAME    Utilise le dossier spécifié pour les traductions\nde Widelands."

# (pofilter) options: Consider translating parameter 'FILENAME' of option '--new_game_from_template'
#: ../../src/wlapplication_messages.cc:116
msgid ""
" --new_game_from_template=FILENAME\n"
"                      Directly create a new singleplayer game\n"
"                      configured in the given file. An example can\n"
"                      be found in `data/templates/new_game_template`."
msgstr " --new_game_from_template=FILENAME\n                      Créé directement une nouvelle partie solo\n                      configurée dans le fichier donné. Un exemple peut\n                      être trouvé dans `data/templates/new_game_template`."

# (pofilter) options: Consider translating parameter 'FILENAME' of option '--scenario'
#: ../../src/wlapplication_messages.cc:122
msgid ""
" --scenario=FILENAME  Directly starts the map FILENAME as scenario\n"
"                      map."
msgstr " --scenario=FILENAME  Démarre directement la carte FILENAME comme carte de\nscénario."

# (pofilter) options: Consider translating parameter 'FILENAME' of option '--loadgame'
#: ../../src/wlapplication_messages.cc:125
msgid " --loadgame=FILENAME  Directly loads the savegame FILENAME."
msgstr " --loadgame=FILENAME  Charge directement la sauvegarde FILENAME."

# (pofilter) options: Consider translating parameter 'FILENAME' of option '--replay'
#: ../../src/wlapplication_messages.cc:126
msgid " --replay=FILENAME    Directly loads the replay FILENAME."
msgstr " --replay=FILENAME    Charge directement le replay FILENAME."

# (pofilter) options: Consider translating parameter 'FILENAME' of option '--script'
#: ../../src/wlapplication_messages.cc:127
msgid ""
" --script=FILENAME    Run the given Lua script after initialization.\n"
"                      Only valid with --scenario, --loadgame, or --editor."
msgstr " --script=FILENAME    Exécute le script LUA donné après l'initialisation.\n                       Seulement valide avec --scenario, --loadgame, ou --editor."

# (pofilter) options: Consider translating parameter '[5' of option '--maxfps'
#: ../../src/wlapplication_messages.cc:150
msgid " --maxfps=[5 ...]     Maximal optical framerate of the game."
msgstr " --maxfps=[5 ...]     Nombre d'images optique maximum du jeux."

# (pofilter) options: Consider translating parameter '[0' of option '--border_snap_distance'
#: ../../src/wlapplication_messages.cc:159
msgid ""
" --border_snap_distance=[0 ...]\n"
"                      Move a window to the edge of the screen\n"
"                      when the edge of the window comes within\n"
"                      this distance from the edge of the screen."
msgstr " --border_snap_distance=[0 ...]\n                      Bouge une fenêtre vers le bord de l'écran\n                      quand le bord de la fenêtre rentre dans\n                      cette distance du bord de l'écran."

# (pofilter) options: Consider translating parameter '[0' of option '--panel_snap_distance'
#: ../../src/wlapplication_messages.cc:172
msgid ""
" --panel_snap_distance=[0 ...]\n"
"                      Move a window to the edge of the panel when\n"
"                      the edge of the window comes within this\n"
"                      distance from the edge of the panel."
msgstr " --panel_snap_distance=[0 ...]\n                      Bouge une fenêtre vers le bord du panneau quand\n                     le bord de la fenêtre entre dans cette distance\n                    du bord du panneau."
