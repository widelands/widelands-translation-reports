msgctxt "message_short_title"
msgid "Building lost!"
msgstr "Bâtiment perdu !"

# (pofilter) endpunc: Different punctuation at the end
#: ../../data/campaigns/atl01.wmf/scripting/texts.lua:486
msgid "We lost a building to the ocean!"
msgstr "Nous avons perdu un bâtiment dans l'océan !"
